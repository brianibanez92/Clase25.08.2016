<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.trabajo.utn.trabajo_practico" >

    <application android:allowBackup="true"
                 android:icon="@mipmap/ic_launcher"
                 android:theme="@style/AppTheme" >

        <uses-permission android:name="android.permission.CAMERA"/>
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
        <activity android:name=".LoginActivity" android:label="@string/lbl_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriasActivity" android:label="@string/lbl_category"/>
        <activity android:name=".RegistroActivity" android:label="@string/lbl_registration"/>
    </application>

</manifest>
